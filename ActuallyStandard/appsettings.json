{
    "Serilog": {
        "MinimumLevel": {
            "Default": "Debug",
            "Override": {
                "Microsoft": "Warning"
            }
        },
        "WriteTo": [
            { "Name": "Console" },
            {
                "Name": "File",
                "Args": {
                    "path": "%TEMP%\\roger-standard-.txt",
                    "rollingInterval": "Day"
                }
            }
        ]
    },
    "Localization": {
        "DefaultCookieName": "UserCulture"
    }
}
